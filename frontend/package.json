{
  "name": "autismdrive",
  "version": "0.0.0",
  "scripts": {
    "backend-reset": "cd .. && ./reset-backend.sh --skip_update && cd ..",
    "build": "ng build",
    "bundle-report": "webpack-bundle-analyzer dist/star-drive/stats-es5.json",
    "e2e": "ng e2e --dev-server-target=",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json,html",
    "lint-and-fix": "tsc --noEmit && eslint . --ext js,ts,json,html --fix",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:watch": "jest -o --watch --config ./jest.config.js",
    "test_headless": "jest --coverage --config ./jest.config.js"
  },
  "engines": {
    "node": "20.12.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.4.0",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.4.0",
    "@angular/compiler": "13.4.0",
    "@angular/core": "13.4.0",
    "@angular/flex-layout": "13.0.0-beta.38",
    "@angular/forms": "13.4.0",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.4.0",
    "@angular/platform-browser-dynamic": "13.4.0",
    "@angular/router": "13.4.0",
    "@angular/youtube-player": "13.3.9",
    "@auth0/angular-jwt": "5.1.2",
    "@ngx-formly/core": "5.12.7",
    "@ngx-formly/material": "5.12.7",
    "@yellowspot/ng-truncate": "2.0.2",
    "core-js": "3.36.1",
    "flat": "5.0.2",
    "geolib": "3.3.4",
    "ngx-markdown": "13.1.0",
    "rfdc": "^1.1.4",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.11",
    "@angular-eslint/builder": "13.5.0",
    "@angular-eslint/eslint-plugin": "13.5.0",
    "@angular-eslint/eslint-plugin-template": "13.5.0",
    "@angular-eslint/schematics": "13.5.0",
    "@angular-eslint/template-parser": "13.5.0",
    "@angular/cli": "13.3.11",
    "@angular/compiler-cli": "13.4.0",
    "@angular/language-service": "13.4.0",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "nightwatch": "3.6.0",
    "@nightwatch/angular": "3.0.0",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "prettier-plugin-organize-imports": "2.3.4",
    "rxjs-compat": "^6.6.3",
    "ts-node": "~9.0.0",
    "typescript": "4.4",
    "webpack-bundle-analyzer": "^3.9.0"
